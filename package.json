{
  "name": "zstd-napi",
  "version": "0.0.7",
  "description": "zstd bindings with N-API",
  "repository": {
    "type": "git",
    "url": "https://github.com/drakedevel/zstd-napi.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node-gyp configure && node-gyp build",
    "clang-format": "clang-format -i src/*",
    "install": "prebuild-install -r napi || node-gyp rebuild",
    "lint": "eslint --ext .js,.ts .",
    "prettier": "prettier -l --write '**/*.{js,json,ts,yml}'",
    "test": "jest",
    "test-coverage": "jest --coverage --coverageReporters=json",
    "typedoc": "typedoc"
  },
  "author": "Andrew Drake <adrake@adrake.org>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@fast-check/jest": "1.8.0",
    "@tsconfig/node18": "18.2.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/jest": "29.5.11",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.6.1",
    "eslint-plugin-tsdoc": "0.2.17",
    "expect-type": "0.17.3",
    "jest": "29.7.0",
    "node-gyp": "10.0.1",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "typedoc": "0.25.6",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@types/node": "^18.19.3",
    "node-addon-api": "^7.0.0",
    "prebuild-install": "^7.1.1"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "files": [
    "binding.d.ts",
    "binding.gyp",
    "binding.js",
    "deps",
    "dist",
    "src"
  ]
}
